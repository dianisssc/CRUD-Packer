<div class="container">
    <div class="row">
        <div class="col-8 offset-2">
            <div class="jumbotron jumbotron-fluid">
                <div class="container">
                    <form class="clearfix mb-4" action="POST">
                        <div class="form-group">
                            <label for="update-name">Edit Name</label>
                            <input type="text" id="update-name" class="form-control" aria-describedby="update-name"
                                placeholder="Enter Example Name">
                        </div>
                        <div class="form-group">

                            <button type="button" id="submit-update" class="btn btn-primary">Submit</button>
                    </form>

                    <div class="row">
                        <div class="col-8 offset-2">
                            <h3>Boxes:</h3>
                            <ul id="example-list" class="list-group">
                                {{#each boxColl}}
                                <li data-id="{{this._id}}" class="list-group-item">
                                    <a href="example/{{this._id}}">{{this.name}}</a>
                                    <button class="btn btn-danger float-right delete">ｘ</button>
                                    <button class="btn btn-danger float-left view">ｘ</button>
                                </li>
                                {{/each}}
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>